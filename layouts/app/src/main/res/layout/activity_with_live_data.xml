<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WithLiveData">

    <Button
        android:id="@+id/with_live_data_change_message_button"
        style="@style/myButton"
        android:layout_marginTop="138dp"
        android:text="change message"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/with_live_data_text"
        style="@style/myText"
        android:layout_marginTop="8dp"
        android:text="message?"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/with_live_data_change_message_button" />

    <Button
        android:id="@+id/with_live_data_change_message_button2"
        style="@style/myButton"
        android:layout_marginTop="8dp"
        android:text="change message2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/with_live_data_text" />

    <TextView
        android:id="@+id/with_live_data_text2"
        style="@style/myText"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/with_live_data_change_message_button2" />
</androidx.constraintlayout.widget.ConstraintLayout>